			<systemPath>${basedir}/jars/jrubyparser-0.5.5-SNAPSHOT.jar</systemPath>
		</dependency>

		<dependency>
			<groupId>net.sf.ehcache</groupId>
			<artifactId>ehcache-core</artifactId>
